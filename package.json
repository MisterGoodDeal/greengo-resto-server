{
  "name": "serial-launcher-server",
  "version": "1.0.0",
  "description": "API Server for the app Serial Laucher",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/index.js -r dotenv/config",
    "start:dev": "nodemon -r dotenv/config",
    "build": "rm -rf build/ && rimraf ./build && tsc && mv ./build/index.js ./build/serial-launcher-server.js && mkdir ./build/images && cp ./src/images/* ./build/images"
  },
  "keywords": [
    "server",
    "express",
    "mysql"
  ],
  "author": "SLTeam",
  "license": "ISC",
  "dependencies": {
    "@types/mysql": "^2.15.18",
    "bcrypt": "^5.0.1",
    "capture-console": "^1.0.1",
    "discord-webhook-node": "^1.1.8",
    "express": "^4.17.1",
    "fetch": "^1.1.0",
    "firebase-admin": "^11.1.0",
    "fs": "^0.0.1-security",
    "imgur": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "serve-favicon": "^2.5.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^15.12.4",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
