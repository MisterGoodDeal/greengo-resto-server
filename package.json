  "license": "ISC",
  "dependencies": {
    "@types/mysql": "^2.15.18",
    "better-sqlite3": "^7.4.5",
    "express": "^4.17.1",
    "fetch": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.4",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
